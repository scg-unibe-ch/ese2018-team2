{
  "name": "unijobs-backend",
  "version": "0.0.1",
  "description": "Back end of unijobs.me",
  "main": "dist/index.js",
  "license": "MIT",
  "private": true,
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "modulePathIgnorePatterns": [
      "/dist/"
    ]
  },
  "scripts": {
    "watch": "concurrently -k \"nodemon dist/index.js\" \"tsc -w\" \"nodemon -w . -e graphql --exec 'yarn run schema:generate'\"",
    "build": "yarn run schema:generate && tsc",
    "schema:generate": "graphqlgen",
    "clean": "rimraf dist **/__generated__ src/**/*.js src/**/*.map",
    "test": "jest"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.7",
    "@types/express-session": "^1.15.11",
    "@types/jest": "^23.3.8",
    "@types/redis": "^2.8.7",
    "@types/ws": "^6.0.1",
    "concurrently": "^4.0.1",
    "jest": "^23.6.0",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "@unijobs/backend-modules-repositories": "^1.0.0",
    "@unijobs/backend-modules-config": "^1.0.0",
    "@unijobs/backend-modules-models": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "connect-redis": "^3.4.0",
    "convict": "^6.2.4",
    "elasticsearch": "^15.2.0",
    "express-session": "^1.15.6",
    "graphql-import": "^0.7.1",
    "graphql-yoga": "^1.16.2",
    "graphqlgen": "^0.4.0",
    "pg": "^7.4.3",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "typeorm": "^0.2.7"
  }
}
